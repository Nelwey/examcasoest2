  <script>
    $(document).ready(function () {
      
      // add row
      $("#btnAddAdministrador").click(function () {
        
        var html = ''
        html += '<div class="row" id="inputFormRow">';
        html += '<div class="col">';
        html += '<select name="admin" class="form-control mb-2">';
        html += '{{#each admin }}';      
        html += '<option value="{{_id}}">{{ nombre }}</option>';       
        html += '{{/each}}';
        html += '</select>';
        html += '</div>';
        html += '<div class="col">';
        html += '<button type="button" class="btn rounded-circle btn-sm btn-danger" id="removeRow">x</button>';
        html += '</div>';
        html += '</div>';
        
          $('#administradoresContenedor').append(html);
        });

        // remove row
        $(document).on('click', '#removeRow', function () {
            $(this).closest('#inputFormRow').remove();
        });

    });
  </script>
